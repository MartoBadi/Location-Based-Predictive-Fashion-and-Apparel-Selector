+                SecurePreferences.sFile.getStringSet(SecurePreferences.encrypt(key), null);
+        if (encryptedSet == null) {
+            return defaultValues;
+        }
+        final Set<String> decryptedSet = new HashSet<String>(encryptedSet.size());
