+            cipher.init(Cipher.ENCRYPT_MODE, new SecretKeySpec(SecurePreferences.sKey, "AES"));
+            return SecurePreferences.encode(cipher.doFinal(cleartext.getBytes("UTF-8")));
+        } catch (Exception e) {
