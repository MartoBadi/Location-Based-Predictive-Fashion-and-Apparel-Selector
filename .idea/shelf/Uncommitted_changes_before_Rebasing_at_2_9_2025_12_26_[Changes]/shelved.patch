+     */
+    protected String getUserId() {
+        return null;
+    }
+
+    /**
+     * Set the connection timeout, override to customize
+     *
+     * @return the timeout in seconds
+     */
+    protected long getHttpClientTimeout() {
+        return 5;
+    }
+}
